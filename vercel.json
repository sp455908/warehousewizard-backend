{
  "version": 2,
  "buildCommand": "npm install && npm run prisma:generate && npm run build",
  "builds": [
    {
      "src": "dist/server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}